<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Main Page{% endblock title %} - {{ 'titles.general.appName'|trans }} {{ version }}</title>
        {% stylesheets
            '../app/Resources/public/bootstrap/css/*'
            '../app/Resources/public/liber/css/*'
        %}
        <link rel="stylesheet" href="{{ asset_url }}">

        {% endstylesheets %}
        <script type="text/javascript" src="{{ asset('bundles/bazingaexposetranslation/js/translator.min.js') }}"></script>
        <script type="text/javascript" src="{{ url('bazinga_exposetranslation_js') }}"></script>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>

        <script src="{{ asset('bundles/obhighcharts/js/highcharts/highcharts.js') }}"></script>
        <script src="{{ asset('bundles/obhighcharts/js/highcharts/modules/exporting.js') }}"></script>

        {% javascripts
            "../app/Resources/public/bootstrap/js/bootstrap.js"
            "../src/Liber/BeleggersBundle/Resources/public/js/*"
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

    </head>
    <body>
        <div id="mainContainer">
            {% block body %}{% endblock %}
            {% block javascripts %}{% endblock %}
        </div>
    </body>
</html>