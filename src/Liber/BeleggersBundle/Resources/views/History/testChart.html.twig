{% extends "LiberBeleggersBundle::layout.html.twig" %}

{% block head %}

{% endblock %}

{% block spanOverride %}
    <div class="span12">
{% endblock %}

{% block content_header %}
<ul class="nav">
        <li><a href="{{ path('liber_beleggers_settings') }}">{{ 'menu.settings'|trans }}</a></li>
        <li><a href="{{ path('liber_beleggers_cashier_register') }}">{{ 'menu.cashier'|trans }}</a></li>
        <li class="active"><a href="{{ path('liber_beleggers_history_test') }}">{{ 'menu.history'|trans }}</a></li>
</ul>
{% endblock %}

{% block title %}{{ 'titles.history.graph'|trans }}{% endblock %}


{% block content %}
    <div class="row-fluid">
        <div class="span12">
            <div class="well">
                <script type="text/javascript">
                    {{ chart(chart) }}
                </script>
                <div id="linechart" style="min-width: 400px; height: 400px; margin: 0 auto"></div>
            </div>
        </div>
    </div>
{% endblock %}

